{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:react-hooks/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
          }      
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-uses-react": 0,
        "react/function-component-definition": 0,
        "react/react-in-jsx-scope": 0,
        "react/prop-types": 0
    },
    "settings": {
        "react": {
          "version": "detect"
        }
      }    
}
